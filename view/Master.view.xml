
<mvc:View
	controllerName="EquipmentHistory.view.Master"
	displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page
		id="master1Page"
		title="{i18n>master1Title}">
		<subHeader>
			<Bar id="master1SearchBar">
				<contentMiddle>
					<SearchField
						id="master1SearchField"
						showRefreshButton="{device>/isNoTouch}"
						placeholder="{i18n>master1SearchPlaceholder}"
						search="onSearch"
						tooltip="{i18n>master1SearchTooltip}"
						width="100%">
					</SearchField>
				</contentMiddle>
			</Bar>
		</subHeader>
		<content>
			<List
				id="master1List"
				items="{/EquipmentSet}"
				mode="{device>/listMode}"
				noDataText="{i18n>masterListNoDataText}"
				select="onSelect"
                growing="true"
				growingScrollToLoad="true">
				<items>
					<ObjectListItem
					    id="master1ListItem"
						type="{device>/listItemType}"
						press="onSelect"						title="{EQUNR}">
						<attributes>
							<ObjectAttribute								text="{EQTYP}"/>
							<ObjectAttribute								text="{INVNR}"/>
						</attributes>
					</ObjectListItem>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>